{
  "name": "x2-vue",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "postinstall": "yarn build",
    "build": "vue-cli-service build",
    "start": "node server.js",
    "test": "vue-cli-service test:unit",
    "watch": "vue-cli-service serve --open --port 6001",
