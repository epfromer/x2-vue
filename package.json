    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/composition-api": "1.0.0-rc.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.2.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-dom": "^4.0.0",
    "prettier": "^2.3.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "vue-cli-plugin-vuetify": "^2.4.0",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.7.2"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.15.0",
    "chart.js": "^2.9.4",
    "core-js": "^3.13.0",
    "echarts": "^5.1.1",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "highcharts": "^9.0.1",
    "highcharts-vue": "^1.3.5",
    "lodash": "^4.17.21",
    "serve-static": "^1.14.1",
    "vue": "^2.6.12",
    "vue-chartjs": "^3.5.1",
    "vue-echarts": "6.0.0-rc.3",
    "vue-router": "^3.5.1",
    "vuetify": "^2.5.0",
    "vuex": "^3.6.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "vue": {
    "transpileDependencies": [
      "vuetify"
    ]
  },
  "prettier": {
    "semi": false,
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "collectCoverage": true,
    "automock": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
